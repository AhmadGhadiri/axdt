<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.axdt.as3.feature"
      label="%featureName"
      version="0.0.82.qualifier"
      provider-name="%providerName">

   <description url="%descriptionUrl">
      %description
   </description>

   <copyright url="%copyrightUrl">
      %copyright
   </copyright>

   <license url="%licenseUrl">
      %license
   </license>

   <url>
      <update label="%updateName" url="http://svn.codespot.com/a/eclipselabs.org/axdt/trunk/site/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.variables"/>
      <import plugin="org.axdt.core.compiler"/>
      <import plugin="org.axdt.core.ui"/>
      <import plugin="org.apache.log4j"/>
      <import plugin="org.eclipse.xtext" version="1.0.1" match="equivalent"/>
      <import plugin="org.axdt.asdoc.runtime"/>
      <import plugin="org.axdt.avm.integration"/>
      <import plugin="org.apache.commons.logging"/>
      <import plugin="org.eclipse.xtext.util"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.antlr.runtime"/>
      <import plugin="org.eclipse.ui.ide" version="3.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtext.builder"/>
      <import plugin="org.eclipse.xtend"/>
      <import plugin="org.eclipse.xpand"/>
      <import plugin="org.eclipse.xtext.ui" version="1.0.1" match="equivalent"/>
      <import plugin="org.eclipse.xtext.ui.shared" version="1.0.1" match="equivalent"/>
      <import plugin="org.axdt.asdoc.ui"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.axdt.core.runtime"/>
   </requires>

   <plugin
         id="org.axdt.as3.debug"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.axdt.as3.runtime"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.axdt.as3.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
